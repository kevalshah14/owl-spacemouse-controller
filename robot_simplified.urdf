<?xml version="1.0" ?>
<robot name="owl_robot">
  
  <!-- World link and virtual joint -->
  <link name="world"/>
  <joint name="virtual_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="world"/>
    <child link="base_link"/>
  </joint>

  <!-- Base Link -->
  <link name="base_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 -0.018 0"/>
      <mass value="2.39"/>
      <inertia ixx="0.006" ixy="0" ixz="0" iyy="0.0095" iyz="0" izz="0.006"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.04"/>
      <geometry>
        <cylinder length="0.08" radius="0.06"/>
      </geometry>
      <material name="light_gray">
        <color rgba="0.9 0.9 0.93 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.04"/>
      <geometry>
        <cylinder length="0.08" radius="0.06"/>
      </geometry>
    </collision>
  </link>

  <!-- Shoulder Link -->
  <link name="shoulder_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0.034 -0.104"/>
      <mass value="4.613"/>
      <inertia ixx="0.0035" ixy="0" ixz="0" iyy="0.0047" iyz="0" izz="0.0048"/>
    </inertial>
    <visual>
      <origin rpy="1.5708 0 0" xyz="0 0 -0.07"/>
      <geometry>
        <cylinder length="0.14" radius="0.05"/>
      </geometry>
      <material name="light_blue">
        <color rgba="0.79 0.82 0.93 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="1.5708 0 0" xyz="0 0 -0.07"/>
      <geometry>
        <cylinder length="0.14" radius="0.05"/>
      </geometry>
    </collision>
  </link>
  
  <!-- Base to Shoulder Joint (BJ) -->
  <joint name="BJ" type="revolute">
    <origin rpy="-3.1416 0 -1.574" xyz="0 0 0.08"/>
    <parent link="base_link"/>
    <child link="shoulder_link"/>
    <axis xyz="0 0 1"/>
    <limit effort="500" lower="-2.0944" upper="2.0944" velocity="0.873"/>
  </joint>

  <!-- Link 1 (Upper Arm) -->
  <link name="link1">
    <inertial>
      <origin rpy="0 0 0" xyz="0.178 0 -0.085"/>
      <mass value="9.431"/>
      <inertia ixx="0.0052" ixy="0" ixz="0" iyy="0.029" iyz="0" izz="0.032"/>
    </inertial>
    <visual>
      <origin rpy="0 1.5708 0" xyz="0.178 0 0"/>
      <geometry>
        <box size="0.356 0.07 0.07"/>
      </geometry>
      <material name="light_blue"/>
    </visual>
    <collision>
      <origin rpy="0 1.5708 0" xyz="0.178 0 0"/>
      <geometry>
        <box size="0.356 0.07 0.07"/>
      </geometry>
    </collision>
  </link>
  
  <!-- Shoulder Joint (SJ) -->
  <joint name="SJ" type="revolute">
    <origin rpy="0.589 1.5675 -0.983" xyz="0 0.067 -0.14"/>
    <parent link="shoulder_link"/>
    <child link="link1"/>
    <axis xyz="0 0 1"/>
    <limit effort="500" lower="-1.6" upper="1.6" velocity="0.873"/>
  </joint>

  <!-- Elbow Link -->
  <link name="elbow_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 -0.029 -0.03"/>
      <mass value="4.613"/>
      <inertia ixx="0.0023" ixy="0" ixz="0" iyy="0.003" iyz="0" izz="0.0036"/>
    </inertial>
    <visual>
      <origin rpy="1.5708 0 0" xyz="0 -0.04 -0.033"/>
      <geometry>
        <cylinder length="0.08" radius="0.045"/>
      </geometry>
      <material name="light_blue"/>
    </visual>
    <collision>
      <origin rpy="1.5708 0 0" xyz="0 -0.04 -0.033"/>
      <geometry>
        <cylinder length="0.08" radius="0.045"/>
      </geometry>
    </collision>
  </link>
  
  <!-- Elbow Joint (EJ) -->
  <joint name="EJ" type="revolute">
    <origin rpy="3.1389 -0.0018 -1.5781" xyz="0.356 0 -0.001"/>
    <parent link="link1"/>
    <child link="elbow_link"/>
    <axis xyz="0 0 1"/>
    <limit effort="500" lower="-1.57" upper="3.14" velocity="0.873"/>
  </joint>

  <!-- Link 2 (Forearm) -->
  <link name="link2">
    <inertial>
      <origin rpy="0 0 0" xyz="0 -0.075 0.138"/>
      <mass value="2.44"/>
      <inertia ixx="0.009" ixy="0" ixz="0" iyy="0.011" iyz="0" izz="0.004"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.138"/>
      <geometry>
        <box size="0.06 0.06 0.276"/>
      </geometry>
      <material name="light_blue"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.138"/>
      <geometry>
        <box size="0.06 0.06 0.276"/>
      </geometry>
    </collision>
  </link>
  
  <!-- Wrist 1 Joint (W1J) -->
  <joint name="W1J" type="revolute">
    <origin rpy="1.5708 -0.0024 0" xyz="0 -0.079 -0.067"/>
    <parent link="elbow_link"/>
    <child link="link2"/>
    <axis xyz="0 0 -1"/>
    <limit effort="500" lower="-2.845" upper="2.845" velocity="0.873"/>
  </joint>

  <!-- Wrist 2/3 Link -->
  <link name="w2w3_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0.03 0.027"/>
      <mass value="3.118"/>
      <inertia ixx="0.0015" ixy="0" ixz="0" iyy="0.0017" iyz="0" izz="0.0019"/>
    </inertial>
    <visual>
      <origin rpy="1.5708 0 0" xyz="0 0.03 0.027"/>
      <geometry>
        <cylinder length="0.06" radius="0.04"/>
      </geometry>
      <material name="light_blue"/>
    </visual>
    <collision>
      <origin rpy="1.5708 0 0" xyz="0 0.03 0.027"/>
      <geometry>
        <cylinder length="0.06" radius="0.04"/>
      </geometry>
    </collision>
  </link>
  
  <!-- Wrist 2 Joint (W2J) -->
  <joint name="W2J" type="revolute">
    <origin rpy="2.745 -1.5667 -1.173" xyz="0 0 0.276"/>
    <parent link="link2"/>
    <child link="w2w3_link"/>
    <axis xyz="0 0 1"/>
    <limit effort="500" lower="-1.6" upper="2.1" velocity="0.873"/>
  </joint>

  <!-- End Effector Link -->
  <link name="end_effector_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.099"/>
      <mass value="0.188"/>
      <inertia ixx="0.00018" ixy="0" ixz="0" iyy="0.00018" iyz="0" izz="0.00026"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.057"/>
      <geometry>
        <cylinder length="0.114" radius="0.025"/>
      </geometry>
      <material name="light_blue"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.057"/>
      <geometry>
        <cylinder length="0.114" radius="0.025"/>
      </geometry>
    </collision>
  </link>
  
  <!-- Wrist 3 Joint (W3J) -->
  <joint name="W3J" type="revolute">
    <origin rpy="-1.258 -1.5668 -0.318" xyz="0 0.06 0.054"/>
    <parent link="w2w3_link"/>
    <child link="end_effector_link"/>
    <axis xyz="0 0 1"/>
    <limit effort="500" lower="-3.1" upper="3.1" velocity="0.873"/>
  </joint>

  <!-- Tool Mount -->
  <link name="tool_mount">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.015"/>
      </geometry>
      <material name="red">
        <color rgba="1 0.2 0.2 1"/>
      </material>
    </visual>
  </link>
  
  <joint name="EEF_to_tool_mount" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.1145"/>
    <parent link="end_effector_link"/>
    <child link="tool_mount"/>
  </joint>

</robot>
